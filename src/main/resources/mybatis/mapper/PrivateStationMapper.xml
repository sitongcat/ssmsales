		<result column="customer_manager" property="customerManager" />
		<result column="memo" property="memo" />
		<result column="customer_address" property="customerAddress" />
		<result column="customer_atten" property="customerAtten" />
		<result column="tel" property="tel" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, customer_id, customer_name, customer_demand_gas, customer_demand_diesel, customer_plan_diesel, customer_area, competition_station, customer_manager, memo, customer_address, customer_atten, tel
